<div>
  <table>
    <thead>
      <th>ID</th>
      <th>Username</th>
      <th>Last Name</th>
      <th>First Name</th>
      <th>Password</th>
      <th>User Type</th>
      <th>Date Created</th>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <td class="eid">{{id}}</td>
        <td class="email">{{email}}</td>
        <td class="last_name">{{last_name}}</td>
        <td class="first_name">{{first_name}}</td>
        <td class="password">{{password}}</td>
        <td class="type">{{type}}</td>
        <td>{{date}}</td>
        <td><button type="button" class="btn btn-secondary edit-btn" name="button"> Edit </button></td>
        <td><button type="button" class="btn btn-secondary delete-btn" name="button"> Delete </button></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<div>
  <button type="button" class="btn btn-dark create-btn" name="button"> Create A User </button>
</div>
